sact.js","original":{"line":61,"column":16}},{"generated":{"line":106,"column":20},"source":"components/Transact.js","original":{"line":34,"column":12}},{"generated":{"line":106,"column":21},"source":"components/Transact.js","original":{"line":33,"column":8}},{"generated":{"line":107,"column":0},"source":"components/Transact.js","original":{"line":71,"column":5}},{"name":"Component","generated":{"line":111,"column":2},"source":"components/Transact.js","original":{"line":5,"column":23}},{"name":"Transact","generated":{"line":113,"column":15},"source":"components/Transact.js","original":{"line":74,"column":15}}],"sources":{"components/Transact.js":"import React, { Component } from \"react\";\nimport { FormGroup, FormControl, Button } from \"react-bootstrap\";\nimport { Link } from \"react-router-dom\";\n\nclass Transact extends Component {\n    state = { recipient: \"\", amount: 0 }\n\n    updateRecipient = event => {\n        this.setState({ recipient: event.target.value });\n    }\n\n    updateAmount = event => {\n        this.setState({ amount: Number(event.target.value) });\n    }\n\n    handleTransaction = () => {\n        const { recipient, amount } = this.state;\n\n        fetch(\"http://localhost:3000/api/transact\", {\n            method: \"POST\",\n            headers: { \"Content-Type\": \"application/json\" },\n            body: JSON.stringify({ recipient, amount })\n        })\n            .then( response => response.json())\n            .then(json => {\n                alert(json.message || json.type);\n            });\n\n    }\n\n    render () {\n\n        return (\n            <div className=\"Transact\">\n                <Link to=\"/\">\n                    My Wallet\n                </Link>\n\n                <h2>\n                    Handling Transactions\n                </h2>\n\n                <FormGroup>\n                    <FormControl \n                        input=\"text\"\n                        placeholder=\"Recipient Address\"\n                        value={ this.state.recipient }\n                        onChange={ this.updateRecipient }\n                    />\n                </FormGroup>\n\n                <FormGroup>\n                    <FormControl \n                        input=\"number\"\n                        placeholder=\"Transaction Value\"\n                        value={ this.state.amount }\n                        onChange={ this.updateAmount }\n                    />\n                </FormGroup>\n\n                <div>\n                    <Button \n                        bsStyle=\"danger\"\n                        onClick={ this.handleTransaction }\n                    >\n                        Confirm\n                    </Button>\n                </div>\n            </div>\n        )\n    };\n}\n\nexport default Transact;"},"lineCount":null}},"error":null,"hash":"1fc13e1c1df1f5ce0791911696d83395","cacheData":{"env":{}}}